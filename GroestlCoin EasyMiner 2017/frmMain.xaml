                <WrapPanel HorizontalAlignment="Center">
                    <RadioButton Name="RbUseElectrum" GroupName="AddressConf" IsChecked="False" Checked="AddressRadioButtons_OnChecked" Margin="0,0,10,0" Visibility="Collapsed">Electrum Address</RadioButton>
                    <RadioButton Name="RbCustomAddress" GroupName="AddressConf" IsChecked="True" Checked="AddressRadioButtons_OnChecked" Visibility="Collapsed">Custom Address</RadioButton>
                </WrapPanel>
                <StackPanel Orientation="Horizontal">
                    
                </StackPanel>
            </StackPanel>
        </GroupBox>
        <Expander x:Name="UxAdvancedSettings" IsExpanded="True" Grid.Row="2"  HorizontalAlignment="Stretch" VerticalAlignment="Top" MaxHeight="200">
            <Expander.Header>
                <Label>Advanced Settings</Label>
            </Expander.Header>
            <ScrollViewer HorizontalAlignment="Stretch" VerticalScrollBarVisibility="Auto">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                    </Grid.RowDefinitions>
                    <GroupBox Grid.Row="0" Header="Mining Pool">
                        <StackPanel HorizontalAlignment="Center">
                            <RadioButton GroupName="Pool" Name="RbUsedwarfPool" IsChecked="True">Use DwarfPool (default)</RadioButton>
                            <RadioButton GroupName="Pool" Name="RbCustomPool">Custom Pool</RadioButton>
                            <WrapPanel>
                                <Label>Address:</Label>
                                <TextBox Height="20" Width="400" Name="TxtPool"></TextBox>
                            </WrapPanel>
                            <WrapPanel>
                                <Label>User Name:</Label>
                                <TextBox  Height="20" Width="150" Name="TxtUsername"></TextBox>
                                <Label>Password:</Label>
                                <TextBox  Height="20" Width="150" Name="TxtPassword"></TextBox>
                            </WrapPanel>

                        </StackPanel>
                    </GroupBox>
                    <GroupBox Grid.Row="1" Header="Donations">
                        <StackPanel Orientation="Vertical">
                            <TextBlock>Donate to the GRS Marketing or Development Funds for...</TextBlock>
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="40"></ColumnDefinition>
                                    <ColumnDefinition Width="100"></ColumnDefinition>
                                    <ColumnDefinition></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <TextBox Name="UxDonationMinutesTxt" Height="25" Width="40">0</TextBox>
                                <Label Grid.Column="1">minutes/hr</Label>
                                <StackPanel Orientation="Horizontal" Grid.Column="2">
                                    <Label FontWeight="Bold">
                                        <Label.Resources>
                                            <Style TargetType="{x:Type Label}" BasedOn="{StaticResource {x:Type Label}}">
                                                <Style.Triggers>
                                                    <DataTrigger Binding="{Binding ElementName=UxFundSwitchRb, Path=IsChecked}" Value="True">
                                                        <Setter Property="Content" Value="Marketing"></Setter>
                                                    </DataTrigger>
                                                    <DataTrigger Binding="{Binding ElementName=UxFundSwitchRb, Path=IsChecked}" Value="False">
                                                        <Setter Property="Content" Value="Development"></Setter>
                                                    </DataTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </Label.Resources>
                                    </Label>
                                    <RadioButton Grid.Column="2" Name="UxFundSwitchRb" Style="{StaticResource AnimatedSwitch}"></RadioButton>
                                </StackPanel>
                                
                            </Grid>
                        </StackPanel>
                        </GroupBox>
                </Grid>
            </ScrollViewer>
        </Expander>
        <ProgressBar Grid.Row="4" Name="ProgressBar"></ProgressBar>
        <StackPanel Grid.Row="5" Width="100" VerticalAlignment="Center">
            <Button Name="BtnStart" Click="BtnStart_OnClick">Start Mining</Button>
        </StackPanel>
    </Grid>
</Window>